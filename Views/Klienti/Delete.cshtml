}



<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Klienti</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayFor(model => model.Emri)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt>
            @Html.DisplayFor(model => model.NumriTel)
        </dt>
